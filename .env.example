# Exemplo de arquivo .env - NÃO COMMITAR COM VALORES REAIS!
# Copie este arquivo para .env e preencha com seus valores reais

# Token do Bot do Telegram
TELEGRAM_TOKEN=seu_token_do_bot_aqui

# Token de acesso do MercadoPago (começa com APP_USR-)
MERCADOPAGO_ACCESS_TOKEN=seu_token_mercadopago_aqui

# Chave pública do MercadoPago (opcional)
PUBLIC_KEY=sua_chave_publica_aqui

# ID do administrador do bot
MY_CHAT_ID=seu_chat_id_aqui

# Link de convite do grupo
GROUP_INVITE_LINK=https://t.me/+seu_link_aqui

# ID do grupo (-100...)
GROUP_CHAT_ID=-100seu_grupo_id_aqui
